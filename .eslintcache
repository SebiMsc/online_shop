e_shop\\online-shop\\src\\components\\Header.js",["112"],"import React, { useState, useContext } from \"react\";\r\nimport { Link } from \"react-router-dom\";\r\nimport CartSvg from \"../static/svg/shopping-cart-solid.svg\";\r\nimport { DataContext } from \"./DataProvider\";\r\n\r\nexport default function Header() {\r\n  const [menu, setMenu] = useState(false);\r\n  const value = useContext(DataContext);\r\n  const [cart] = value.cart;\r\n\r\n  const toggleMenu = () => {\r\n    setMenu(!menu);\r\n  };\r\n  const styleMenu = {\r\n    left: menu ? 0 : \"-100%\",\r\n  };\r\n\r\n  return (\r\n    <header>\r\n      <div className=\"logo\">\r\n        <h1>\r\n          <Link to=\"/products\">Online Shop</Link>\r\n        </h1>\r\n      </div>\r\n      <ul style={styleMenu}>\r\n        <li>\r\n          <Link to=\"/\">Home</Link>\r\n        </li>\r\n        <li>\r\n          <Link to=\"/products\">Products</Link>\r\n        </li>\r\n        {/* <li>\r\n          <Link to=\"/products/:id\" exact>\r\n            Details\r\n          </Link>\r\n        </li> */}\r\n        <li>\r\n          <Link to=\"/about\">About</Link>\r\n        </li>\r\n        <li>\r\n          <Link to=\"/admin\">Admin</Link>\r\n        </li>\r\n        <li>\r\n          <Link to=\"/login\">Login/Logout</Link>\r\n        </li>\r\n        <li>\r\n          <i className=\"fas fa-times\"></i>\r\n        </li>\r\n        <li onClick={toggleMenu}></li>\r\n      </ul>\r\n      <div className=\"menu\" onClick={toggleMenu}>\r\n        <i className=\"fas fa-bars\"></i>\r\n      </div>\r\n      <div className=\"cart-icon\">\r\n        <span> {cart.length}</span>\r\n        <Link to=\"/cart\">\r\n          <img src={CartSvg} width=\"30\" />\r\n        </Link>\r\n      </div>\r\n    </header>\r\n  );\r\n}\r\n","C:\\Users\\sebastian.moisescu\\Desktop\\ReactProjects\\online_shop\\online-shop\\src\\components\\DataProvider.js",["113","114"],"import React, { createContext, useState, useEffect } from \"react\";\r\nimp